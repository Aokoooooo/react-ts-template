{
  "author": {
    "name": "Aoko",
    "email": "hejunqin233@gmail.com"
  },
  "name": "react-ts-template",
  "version": "1.0.0",
  "description": "react template with typescript",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Aokoooooo/react-ts-template"
  },
  "bugs": {
    "url": "https://github.com/Aokoooooo/react-ts-template/issues"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "postBuild": "node ./scripts/postBuild.js",
    "lint": "cross-env yarn run prettier && yarn run lint:fix",
    "prettier": "prettier --write src/**/*.{js,ts,tsx,css,less,json}",
    "lint:fix": "yarn run stylelint:fix && yarn run tslint:fix",
    "stylelint": "cross-env stylelint 'src/**/*.less' --syntax less",
    "stylelint:fix": "cross-env stylelint --fix 'src/**/*.less' --syntax less",
    "tslint": "cross-env tslint 'src/**/*.ts*'",
    "tslint:fix": "cross-env tslint --fix 'src/**/*.ts*'"
  },
  "dependencies": {
    "@loadable/component": "^5.10.2",
    "antd": "^3.20.3",
    "aqua-actions": "^0.1.5",
    "axios": "^0.19.0",
    "classnames": "^2.2.6",
    "history": "^4.9.0",
    "lodash": "^4.17.14",
    "moment": "^2.24.0",
    "rc-animate": "^2.9.2",
    "react": "^16.8.6",
    "react-container-query": "^0.11.0",
    "react-dom": "^16.8.6",
    "react-media": "^1.9.2",
    "react-redux": "^7.1.0",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.0.1",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "typescript": "^3.4.5"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.9",
    "@types/history": "^4.7.2",
    "@types/jest": "^24.0.15",
    "@types/loadable__component": "^5.10.0",
    "@types/lodash": "^4.14.136",
    "@types/node": "^12.6.8",
    "@types/react": "16.8.23",
    "@types/react-dom": "16.8.4",
    "@types/react-redux": "^7.1.1",
    "@types/react-router": "^5.0.3",
    "@types/react-router-dom": "^4.3.4",
    "babel-plugin-import": "^1.12.0",
    "console-polyfill": "^0.3.0",
    "core-js": "^3.1.4",
    "cross-env": "^5.2.0",
    "customize-cra": "^0.4.1",
    "es3ify-webpack-plugin": "^0.1.0",
    "es5-shim": "^4.5.13",
    "es6-promise": "^4.2.8",
    "fetch-ie8": "^1.5.0",
    "http-proxy-middleware": "^0.19.1",
    "husky": "^3.0.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "lint-staged": "^9.2.0",
    "prettier": "^1.18.2",
    "react-app-rewired": "^2.1.3",
    "redux-devtools": "^3.5.0",
    "redux-devtools-extension": "^2.13.8",
    "stylelint": "^10.1.0",
    "stylelint-config-css-modules": "^1.4.0",
    "stylelint-config-prettier": "^5.2.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.1.0",
    "stylelint-order": "^3.0.1",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "cross-env lint-staged"
    }
  },
  "lint-staged": {
    "**/*.less": [
      "stylelint --fix --syntax less",
      "git add"
    ],
    "**/*.{ts,tsx}": [
      "tslint --fix",
      "git add"
    ],
    "**/*.{js,ts,tsx,css,less,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
