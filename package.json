{
  "name": "react-ts-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "prettier": "prettier --write src/**/*.{js,ts,tsx,css,less,json}",
    "lint:fix": "yarn run stylelint:fix && yarn run tslint:fix",
    "stylelint": "cross-env stylelint 'src/**/*.less' --syntax less",
    "stylelint:fix": "cross-env stylelint --fix 'src/**/*.less' --syntax less",
    "tslint": "cross-env tslint 'src/**/*.ts*'",
    "tslint:fix": "cross-env tslint --fix 'src/**/*.ts*'"
  },
  "dependencies": {
    "@loadable/component": "^5.9.0",
    "antd": "^3.17.0",
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "history": "^4.9.0",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "prettier": "^1.17.0",
    "rc-animate": "^2.8.2",
    "react": "^16.8.6",
    "react-container-query": "^0.11.0",
    "react-dom": "^16.8.6",
    "react-media": "^1.9.2",
    "react-redux": "^7.0.3",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "react-scripts": "3.0.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "typescript": "3.4.5"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.7",
    "@types/history": "^4.7.2",
    "@types/jest": "24.0.12",
    "@types/loadable__component": "^5.9.0",
    "@types/lodash": "^4.14.124",
    "@types/moment": "^2.13.0",
    "@types/node": "12.0.0",
    "@types/react": "16.8.16",
    "@types/react-dom": "16.8.4",
    "@types/react-redux": "^7.0.8",
    "@types/react-router": "^5.0.0",
    "@types/react-router-dom": "^4.3.3",
    "babel-plugin-import": "^1.11.0",
    "cross-env": "^5.2.0",
    "customize-cra": "^0.2.12",
    "husky": "^2.2.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "lint-staged": "^8.1.6",
    "react-app-rewired": "^2.1.3",
    "redux-devtools": "^3.5.0",
    "redux-devtools-extension": "^2.13.8",
    "stylelint": "^10.0.1",
    "stylelint-config-css-modules": "^1.4.0",
    "stylelint-config-prettier": "^5.1.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.1.0",
    "stylelint-order": "^3.0.0",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "cross-env lint-staged"
    }
  },
  "lint-staged": {
    "**/*.less": [
      "stylelint --fix --syntax less",
      "git add"
    ],
    "**/*.{ts,tsx}": [
      "tslint --fix",
      "git add"
    ],
    "**/*.{js,ts,tsx,css,less,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
